{
  "name": "visa-direct-sdk",
  "version": "0.3.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run --reporter=default --coverage",
    "test:watch": "vitest",
    "test:pii-scanning": "vitest run tests/no-pii-ci.test.ts --reporter=default",
    "test:fail-closed": "vitest run tests/mle-jwks-fail-closed.test.ts --reporter=default",
    "test:ci": "npm run test && npm run test:pii-scanning && npm run test:fail-closed",
    "lint": "eslint .",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.684.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-trace-base": "^1.23.0",
    "@opentelemetry/sdk-trace-node": "^1.23.0",
    "@types/ioredis": "^4.28.10",
    "axios": "1.7.7",
    "ioredis": "^5.8.1",
    "jose": "^5.10.0"
  },
  "devDependencies": {
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.1",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5",
    "vitest": "^2.0.0"
  }
}
